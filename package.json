{
	"name": "my-portfolio",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"lint": "eslint ."
	},
	"dependencies": {
		"@astrojs/tailwind": "3.0.1",
		"astro": "2.0.13",
		"astro-icon": "^0.8.0",
		"tailwindcss": "3.2.7"
	},
	"devDependencies": {
		"eslint-plugin-astro": "0.23.0",
		"standard": "17.0.0"
	},
	"eslintConfig": {
		"extends": [
			"./node_modules/standard/eslintrc.json",
			"plugin:astro/recommended"
		],
		"overrides": [
			{
				"files": [
					"*.astro"
				],
				"parser": "astro-eslint-parser",
				"parserOptions": {
					"extraFileExtensions": [
						".astro"
					]
				},
				"rules": {}
			}
		]
	}
}
